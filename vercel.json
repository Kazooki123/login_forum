{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.0"
        }
    },
    "routes": [
        {
            "src": "/",
            "dest": "/api/registration.php"
        },
        {
            "src": "/login",
            "dest": "/api/login.php"
        },
        {
            "src": "/index",
            "dest": "/api/index.php"
        },
        {
            "src": "/logout",
            "dest": "/api/logout.php"
        }
    ]
}